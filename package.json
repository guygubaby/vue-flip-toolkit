    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.16.3",
    "flip-toolkit": "^7.0.14",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "unocss": "^0.37.4",
    "unplugin-auto-import": "^0.8.7",
